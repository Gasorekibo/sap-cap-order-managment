{
  "compilerOptions": {
      "target": "ES2022",
      "module": "nodenext",
      "outDir": "./dist",
      "rootDir": "./",
      "baseUrl": "./",
      "allowJs": true,
      "esModuleInterop": true,
      "inlineSources": true,
      "inlineSourceMap": true,
      "moduleResolution": "nodenext",
      "paths": {
          "#cds-models/*": [
              "./@cds-models/*"
          ]
      }
  },
  "include": [
      "./srv/**/*"
  ]
}